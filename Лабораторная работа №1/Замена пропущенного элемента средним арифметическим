numbers = [2, -93, -2, 8, None, -44, -1, -85, -14, 90, -22, -90, -100, -8, 38, -92, -45, 67, 53, 25]

# TODO заменить значение пропущенного элемента средним арифметическим
count = 0
for i in numbers:
    if i is not None:
        count += i

numbers[4] = (count / len(numbers))
print(f'Измененный список: {numbers}')

